        </div>

        <!-- MAIN: Event list -->
        
        <div class="col-sm-12 col-md-6 nopadding main">
          <div id="events">
            {{#allEvents}}
            <div class="event">
              <p class="event-details">
                {{venue_address}} | {{time}}
              </p>
              <h2 class="subheader">
                <a href="http://facebook.com/{{id}}" target="_blank">   
                  {{name}}
                </a>
              </h2>
              <p class="event-venue-name">
                {{venue_name}}
              </p>
            </div>
            {{/allEvents}}
          </div>
        </div>

