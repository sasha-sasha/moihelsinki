<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- The above 3 meta tags *must* come first in the head -->
  <!-- any other head content must come *after* these tags -->

  <title>{{ title }}</title>

  <!-- Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Gentium+Basic&subset=latin,latin-ext"
  rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Karla:700,400&subset=latin,latin-ext"
  rel="stylesheet" type="text/css">

  <!-- Bootstrap -->
  <link href="css/bootstrap.min.css" rel="stylesheet">

  <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
  <link href="css/ie10-viewport-bug-workaround.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="css/dashboard.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="css/styles.css" rel="stylesheet">


  <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
</head>
<body class="background-white color-orange">

  <!-- Fixed navbar -->
  <nav class="navbar navbar-fixed-top">

    <div class="container-fluid nopadding">
      <div class="row nopadding buttons">

        <div class="menu-button col-xs-4 col-md-3 nopadding">
          <!-- Image: About/Logo -->
          <a class="button" data-toggle="collapse" href="#collapseExample3" aria-expanded="false" aria-controls="collapseExample">
            <img src="images/logo.svg">
          </a>
        </div>
        
    
        <div class="menu-button col-xs-4 col-md-6 nopadding">
          <!-- Spacer -->
        </div>

        <div class="menu-button col-xs-4 col-md-3 nopadding">
          <!-- Image: Filters-->
          <a class="button" data-toggle="collapse" href="#collapseExample4" aria-expanded="false" aria-controls="collapseExample">
            <img src="images/filters.svg">
          </a>
        </div>

      </div>

      <div class="row nopadding">

       <div class="col-md-3 nopadding">

          <!-- About -->

          <div class="collapse overlay" id="collapseExample3" >
            <img src="images/about_box.svg" width="80%" style="margin-top:20%">
          </div>
       </div>

        <div class="col-md-6 nopadding">
        </div>

       <div class="col-md-3 nopadding">

        <!-- Filters -->

        <div class="collapse overlay" id="collapseExample4" >
          <!-- Location: address input -->       
          <!--<input type="text" placeholder="Enter address..">-->
          <img src="images/filter_box.svg" width="80%"  style="margin-top:10%">
        </div>

       </div>


      </div>



    </div>

  </nav>

  <!-- MAIN LANE -->

  <div class="container-fluid nopadding">
    <div class="row nopadding">
      <!-- Spacer -->

      <div class="col-md-3 nopadding">  
      </div>

      <!-- MAIN: Event list -->
      
      <div class="col-md-6 nopadding main">
        <div id="events">
          {{#allEvents}}
          <div class="event">
            <p class="event-details">
              {{venue_address}} | {{time}}
            </p>
            <h2 class="subheader">
              <a href="http://facebook.com/{{id}}" target="_blank">   
                {{name}}
              </a>
            </h2>
            <p class="event-venue-name">
              {{venue_name}}
            </p>
          </div>
          {{/allEvents}}
        </div>
      </div>

      <!-- Spacer -->

      <div class="col-md-3 nopadding">  
      </div>
    </div>
  </div>

  <!-- /MAIN LANE-->

  </div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>

<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>

<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
<script src="js/ie10-viewport-bug-workaround.js" type="text/javascript"></script>
</body>
</html>